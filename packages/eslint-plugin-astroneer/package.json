{
  "name": "eslint-plugin-astroneer",
  "version": "1.4.14-canary.0",
  "description": "ESLint plugin for Astroneer",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "directory": "packages/eslint-plugin",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@typescript-eslint/utils": "^7.10.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && swc -d dist src"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./use-at-your-own-risk": "./dist/rules.js"
  }
