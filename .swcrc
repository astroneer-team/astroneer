{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false,
      "decorators": false
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "externalHelpers": false,
    "keepClassNames": false,
    "target": "esnext"
  },
  "module": {
    "type": "commonjs"
  }
}
