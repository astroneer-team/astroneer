{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "1.4.8",
  "packages": ["packages/*"],
  "command": {
    "run": {
      "sort": true,
      "private": false
    },
    "publish": {
      "registry": "https://registry.npmjs.org/",
      "distTag": "latest"
    },
    "version": {
      "gitTagVersion": true
    }
  }
}
