{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "1.4.5",
  "packages": [
    "packages/*"
  ],
  "command": {
    "run": {
      "sort": true,
      "private": false
    },
    "publish": {
      "registry": "https://registry.npmjs.org/",
      "distTag": "latest",
      "yes": true
    },
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "yes": true,
      "gitTagVersion": true
    }
  }
}
